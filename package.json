{
    "name": "vscode-rust-template-literals",
    "displayName": "Rust Template Literals",
    "description": "Syntax highlighting for rust in template literals",
    "version": "0.1.0",
    "publisher": "Dan Steren",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/dansteren/vscode-rust-template-literals.git"
    },
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "grammars": [
            {
                "injectTo": [
                    "source.js",
                    "source.js.jsx",
                    "source.jsx",
                    "source.ts",
                    "source.tsx"
                ],
                "scopeName": "inline.lit-rust",
                "path": "./syntaxes/lit-rust.json",
                "embeddedLanguages": {
                    "meta.embedded.block.rust": "rust"
                }
            }
        ]
    }
}
